<Window x:Class="GamingAssistant.Loader"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GamingAssistant"
        mc:Ignorable="d"
        Title="Loader" Height="200" Width="200" Opacity="100" WindowStartupLocation="CenterScreen" AllowsTransparency="True" WindowStyle="None">
    <Window.Background>
        <ImageBrush/>
    </Window.Background>
    <Grid>
        <ProgressBar  Style="{StaticResource MaterialDesignCircularProgressBar}"  Value="35"  IsIndeterminate="True" Width="100" Height="100" />
    </Grid>
</Window>
